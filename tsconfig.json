{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "module": "CommonJS",
    "types": ["cypress", "node"],
    "sourceMap": false,
    "paths": {
      "@/*": ["src/*"]
    },
    "target": "ES2021"
  },
  "exclude": ["node_modules"],
  "include": ["../cypress.config.ts"]
}
