{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "module": "CommonJS",
    "types": ["cypress", "node"],
    "target": "ES2021",
    "baseUrl": "./",
    "paths": {
      "@fixtures/*": ["cypress/fixtures/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["cypress.config.ts", "cypress/**/**/*.ts"]
}
